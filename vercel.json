{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/The-Garrison-System/browser",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://tgs-backend-u5xz.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
