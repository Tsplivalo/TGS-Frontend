{
  "ci": {
    "collect": {
      "staticDistDir": "dist/The-Garrison-System",
      "url": ["/index.html"],
      "numberOfRuns": 1
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.7 }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
